<div id="<%= dom_id building %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Building name:</strong>
    <%= building.building_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Address:</strong>
    <%= building.address %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Location:</strong>
    <%= building.location %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Other description:</strong>
    <%= building.other_description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Register status:</strong>
    <%= building.register_status? ? "Yes" : "No" %>
  </div>
</div>
